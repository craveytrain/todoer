{
	"name": "todoer",
	"private": true,
	"description": "A simple todo app written in gatsby",
	"version": "0.1.0",
	"author": "Mike Cravey <techie@craveytrain.com>",
	"dependencies": {
		"apollo-boost": "^0.4.4",
		"gatsby": "^2.13.83",
		"gatsby-plugin-manifest": "^2.2.9",
		"gatsby-plugin-offline": "^2.2.10",
		"gatsby-plugin-react-helmet": "^3.1.5",
		"isomorphic-fetch": "^2.2.1",
		"react": "^16.9.0",
		"react-apollo": "^3.0.1",
		"react-dom": "^16.9.0",
		"react-helmet": "^5.2.1"
	},
	"devDependencies": {
		"husky": "^3.0.4",
		"npm-check": "^5.9.0",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"format": "prettier --write \"{{src,functions}/**/*.{js,jsx},*.{js,json}}\"",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
		"postinstall": "cd functions && npm ci",
		"upgrade-interactive": "npm-check --update"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gatsbyjs/gatsby-starter-default"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
